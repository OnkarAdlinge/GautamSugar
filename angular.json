{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Gautamsugar": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/gautamsugar/browser",
            "index": "src/index.html",
            "browser": "src/main.ts",         
            "tsConfig": "tsconfig.app.json",
            "polyfills": ["zone.js"],
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1MB",
                  "maximumError": "1MB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Gautamsugar:build:production"
            },
            "development": {
              "buildTarget": "Gautamsugar:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "build-ssr": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/gautamsugar/server",
            "browser": "src/main.server.ts",   
            "tsConfig": "tsconfig.server.json",
            "index": "src/index.html"
          },
          "configurations": {
            "production": {
              "outputHashing": "media"
            },
            "development": {
              "sourceMap": true,
              "optimization": false
            }
          }
        },
        "serve-ssr": {
          "builder": "@angular-devkit/build-angular:ssr-dev-server",
          "options": {
            "browserTarget": "Gautamsugar:build",
            "serverTarget": "Gautamsugar:build-ssr"
          },
          "configurations": {
            "development": {
              "browserTarget": "Gautamsugar:build:development",
              "serverTarget": "Gautamsugar:build-ssr:development"
            },
            "production": {
              "browserTarget": "Gautamsugar:build:production",
              "serverTarget": "Gautamsugar:build-ssr:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@angular-devkit/build-angular:prerender",
          "options": {
            "browserTarget": "Gautamsugar:build:production",
            "serverTarget": "Gautamsugar:build-ssr:production",
            "routes": ["/"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "5fc60c8c-df08-4ee0-9af5-991beca81b65"
  }
}
